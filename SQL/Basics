/* Extract all from Table
SELECT * From Census;

/* SQL query that defines a table "Census" with 5 columns: 
"id" of type INTEGER, "name" of type VARCHAR(20), 
"birth_place_latitude" of type REAL, 
"year_income" of type DECIMAL(20,3), 
and "is_male" of type BOOLEAN. */
CREATE TABLE Census (
    id INTEGER,
    name VARCHAR(20),
    birth_place_latitude REAL,
    year_income DECIMAL(20,3),
    is_male BOOLEAN
);

/* Hello, World in SQL (an expression that we want to evaluate)
SELECT 'Hello, World!';

/* Type casting
SELECT CAST(256 AS DECIMAL(10, 2));

concat('Hyper', 'skill') /* returns 'Hyperskill'
char_length('apple') /* equals 5
reverse('Madam, I''m Adam') /* returns 'madA m''I ,madaM'
replace('abracadabra', 'bra', '') /* returns 'acada'
substr('Hello, data!', 8, 4) /* returns 'data'

((true AND true) AND (NOT false)) OR false /* evaluated as true
