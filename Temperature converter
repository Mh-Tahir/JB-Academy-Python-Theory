# Basic temperature converter from Fahrenheit to Celsius and vice versa

def fahrenheit_to_celsius(temps_f):
    temps_c = (temps_f - 32) * 5 / 9
    return round(temps_c, 2)

def celsius_to_fahrenheit(temps_c):
    temps_f = temps_c * 9 / 5 + 32
    return round(temps_f, 2)

def main():
    temperature, unit = input().split()
    t = int(temperature)
    if unit == 'F':
        print(fahrenheit_to_celsius(t), 'C')
    elif unit == 'C':
        print(celsius_to_fahrenheit(t), 'F')
