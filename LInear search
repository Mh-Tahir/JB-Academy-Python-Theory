# if not sorted
def search(elements, target):
    index = -1
 
    for i, elem in enumerate(elements):
        if elem == target:
            index = i
            break
 
    return index

# if sorted
def search(elements, target):
    index = -1
 
    for i, elem in enumerate(elements):
        if elem == target:
            index = i
            break
 
        if elem > target:
            break
 
    return index

# k-th occurrence
def kth(numbers, target, k):
    x = 0
    for i, n in enumerate(numbers):
        if n == target:
            x += 1
            if x == k:
                return i
    else:
        return -1


numbers = [int(i) for i in input().split()]
target = int(input())
k = int(input())
print(kth(numbers, target, k))

# Count occurrences
def count(numbers, target):
    x = 0
    for n in numbers:
        if n == target:
            x += 1
    return x


numbers = [int(i) for i in input().split()]
target = int(input())
print(count(numbers, target))

